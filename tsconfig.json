{
    "compileOnSave": false,
    "compilerOptions":
    {
        "module": "ES6",
        "moduleResolution": "node",
        "target": "ES6",
        "sourceMap": true,
        "jsx" : "react",
        "jsxFactory": "Snabbdom.createElement",
        "noImplicitReturns": true,
        "strict": true,
        "allowJs": true,
        "lib": ["ES6", "ES5", "DOM"]
    },
    "exclude": [
        "node_modules"
    ]
}
